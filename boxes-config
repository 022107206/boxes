BOX xes

author "Joan G. Stark <spunk1111@juno.com>"
revision "1.0"
revdate "April 09, 1999 (Friday, 20:05h)"
created "April 09, 1999 (Friday, 20:05h)"

sample {
            <\\/><\\/><\\/>
        <\\/></\\></\\></\\><\\/>
        </\\>            </\\>
     <\\/>                  <\\/>
     </\\>                  </\\>
    <\\/>                    <\\/>
    </\\>                    </\\>
     <\\/>                  <\\/>
     </\\>                  </\\>
        <\\/>            <\\/>
        </\\><\\/><\\/><\\/></\\>
    jgs     </\\></\\></\\>
}

shapes {
    nw  ("     ", "    <", "    <")
    nnw ("   ", "\\/>", "/\\>")
    n   ("<\\/>", "</\\>", "    ")
    nne ("   ", "<\\/", "</\\")
    ne  ("     ", ">    ", ">    ")
    wnw (" <\\/>", " </\\>")
    w   ("<\\/> ", "</\\> ")
    wsw (" <\\/>", " </\\>")
    sw  ("    <", "    <", "     ")
    ssw ("\\/>", "/\\>", "   ")
    s   ("    ", "<\\/>", "</\\>")
    sse ("<\\/", "</\\", "   ")
    se  (">    ", ">    ", "     ")
    ese ("<\\/> ", "</\\> ")
    e   (" <\\/>", " </\\>")
    ene ("<\\/> ", "</\\> ")
}

