BOX scroll

revision "1.0"
revdate  "March 17, 1999 (Wednesday, 17:04h)"
created  "March 17, 1999 (Wednesday, 17:04h)"

sample {
     / ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ \\  
    |   / ~ \\                                           / ~ \\   |
    |\\  \\    |                                         |    /  /|
    |  \\    /|                                         |\\    /  |
    |    ~~  |                                         |  ~~    |
    |        |                                         |        |
    |        |                                         |        |
    |        |                                         |        |
    |        |                                         |        |
    |        |                                         |        |
     \\       |~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~|       /
       \\    /                                           \\    /
         ~~                                               ~~
}

shapes {
    nw  (" / ~~~~~~~")
    n   ("~")
    ne  ("~~~~~~~ \\ ")
    ene (" / ~ \\   |", "|    /  /|", "|\\    /  |", "|  ~~    |")
    wnw ("|   / ~ \\ ", "|\\  \\    |", "|  \\    /|", "|    ~~  |")
    e   ("|        |")
    se  ("|       / ", " \\    /   ", "   ~~     ")
    s   ("~"," "," ")
    sw  (" \\       |", "   \\    / ", "     ~~   ")
    w   ("|        |")
}

# offsets {
#     ne-ene -1
#     ese-se  1
#     sse-s   2
#     ssw-sw -2
#     wnw-nw  1
# }

eLAStic (n, s, e, w)

END scroll


