BOX java-cmt

author "Thomas Jensen <tsjensen@stud.informatik.uni-erlangen.de>"
revision "1.0"
revdate "June 17, 1999 (Thursday, 19:59h)"
created "June 17, 1999 (Thursday, 19:59h)"

sample {
    //
    // regular Java
    // comments
    //
}

shapes {
    nw ("  ") n (" ") ne (" ")
     w ("//")          e (" ")
    sw ("  ") s (" ") se (" ")
}

elastic (n,e,s,w)

END java-cmt


