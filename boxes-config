BOX cat

author "Joan G. Stark <spunk1111@juno.com>"
revision "1.0"
revdate "April 09, 1999 (Friday, 19:45h)"
created "April 09, 1999 (Friday, 19:45h)"

sample {
                  /\\             /\\
                 |`\\\\_,--="=--,_//`|
                 \\ ."  :'. .':  ". /
                ==)  _ :  '  : _  (==
                  |>/O\\   _   /O\\<|
                  | \\-"~` _ `~"-/ |
                 >|`===. \\_/ .===`|<
       jgs .-"-.   \\==='  |  '===/   .-"-.
    .-----{'. '`\}---\\,  .-'-.  ,/---{.'. '\}-----.
     )    `"---"`     `~-===-~`     `"---"`    (
    (                 joan  stark               )
     )           < spunk1111@juno.com >        (
    (              ASCII ART GALLERY:           )
     ) < http://www.geocities.com/SoHo/7373/ > (
    '-------------------------------------------'
}

shapes {
    nnw (" ", " ", " ", " ", " ", " ", " ", " ", "-", " ")
    nne (" ", " ", " ", " ", " ", " ", " ", " ", "-", " ")
    nw  ("   ", "   ", "   ", "   ", "   ", "   ", "   ", "   ", ".--", " ) ")
    ne  ("   ", "   ", "   ", "   ", "   ", "   ", "   ", "   ", "--.", " ( ")
    w   ("(  ", " ) ")
    e   ("  )", " ( ")
    sw  ("'--")
    se  ("--'")
    s   ("-")
    n  ("        /\\             /\\        ",
        "       |`\\\\_,--=\"=--,_//`|       ",
        "       \\ .\"  :'. .':  \". /       ",
        "      ==)  _ :  '  : _  (==      ",
        "        |>/O\\   _   /O\\<|        ",
        "        | \\-\"~` _ `~\"-/ |        ",
        "       >|`===. \\_/ .===`|<       ",
        " .-\"-.   \\==='  |  '===/   .-\"-. ",
        "{'. '`}---\\,  .-'-.  ,/---{.'. '}",
        "`\"---\"`     `~-===-~`     `\"---\"`")
}

elastic (nnw, nne,e,s,w)

END cat


