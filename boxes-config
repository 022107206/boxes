BOX columns2

revision "1.0"
revdate  "March 29, 1999 (Monday, 18:19h)"
created  "March 29, 1999 (Monday, 18:19h)"
author   "Thomas Jensen <tsjensen@stud.informatik.uni-erlangen.de>"

sample {
     __^__                 __^__
    ( ___ )---------------( ___ )
     | / |                 | \\ |
     | / |                 | \\ |
    /| / |\\               /| \\ |\\
    \\| / |/               \\| \\ |/
     | / |                 | \\ |
     |___|                 |___|
    (_____)---------------(_____)
}

shapes {
    nw  (" __^__ ", "( ___ )")
    nnw (" ","-")
    ne  (" __^__ ", "( ___ )")

    ene (" | \\ | ")
    e   ("/| \\ |\\", "\\| \\ |/")
    ese (" | \\ | ")

    wnw (" | / | ")
    w   ("/| / |\\", "\\| / |/")
    wsw (" | / | ")

    sw  (" |___| ", "(_____)")
    s   (" ","-")
    se  (" |___| ","(_____)")
}

elastic (nnw, ene, ese, wsw, wnw, s)

END columns2


