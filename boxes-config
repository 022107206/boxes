BOX spring

author "Joan G. Stark <spunk1111@juno.com>"
revision "1.0"
revdate "April 09, 1999 (Friday, 15:53h)"
created "April 09, 1999 (Friday, 15:53h)"

sample {
          ,
      /\\^/`\\             
     | \\/   |            
     | |    |               SPRING IS IN THE AIR!              jgs
     \\ \\    /                                                _ _
      '\\\\//'                                               _{ ' \}_
        ||                      joan stark                { `.!.` \}
        ||                  <spunk1111@juno.com>          ',_/Y\\_,'
        ||  ,                                               {_,_\}
    |\\  ||  |\\                                                |
    | | ||  | |              ASCII ART GALLERY:             (\\|  /)
    | | || / /      <http://www.geocities.com/SoHo/7373/>    \\| //
     \\ \\||/ /                                                 |//
      `\\\\//`   \\\\   \\./    \\\\ /     //    \\\\./   \\\\   //   \\\\ |/ /
     ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^
}

shapes {
    nw  ("      ,    ")
    wnw ("  /\\^/`\\   ",
         " | \\/   |  ",
         " | |    |  ",
         " \\ \\    /  ",
         "  '\\\\//'   ")
    w   ("    ||     ")
    wsw ("    ||  ,  ",
         "|\\  ||  |\\ ",
         "| | ||  | |",
         "| | || / / ",
         " \\ \\||/ /  ")
    sw  ("  `\\\\//`   ",
         " ^^^^^^^^^^")
    ne  ("         ")
    ene ("         ",
         "         ",
         "     jgs ",
         "   _ _   ",
         " _{ ' }_ ",
         "{ `.!.` }",
         "',_/Y\\_,'",
         "  {_,_}  ")
    e   ("    |    ")
    ese ("  (\\|  /)",
         "   \\| // ",
         "    |//  ")
    se  (" \\\\ |/ / ",
         "^^^^^^^^^")
    n   (" ")
    s   ("\\   \\./    \\",
         "^^^^^^^^^^^^")
}

elastic (n,s,e,w)

END spring


