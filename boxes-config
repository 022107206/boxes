BOX columns

revision "1.0"
revdate  "March 17, 1999 (Wednesday, 15:27h)"
created  "March 17, 1999 (Wednesday, 15:27h)"

sample {
     __^__                 __^__
    ( ___ )---------------( ___ )
     | / |                 | \\ |
     | / |                 | \\ |
     | / |                 | \\ |
     |___|                 |___|
    (_____)---------------(_____)
}

shapes {
    nw  (" __^__ ", "( ___ )")
    nnw (" ","-")
    ne  (" __^__ ", "( ___ )")
    e   (" | / | ")
    w   (" | \\ | ")
    wsw (" |___| ")
    ese (" |___| ")
    sw  ("(_____)")
    s   ("-")
    se  ("(_____)")
}

# offsets {
#     nw-nnw  1
#     nne-ne -1
#     ne-ene  1
#     ese-se -1
#     sw-wsw  1
# }

elastic (nnw, e, w, s)

END columns


