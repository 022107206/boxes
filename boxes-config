BOX columns

revision "1.0"
revdate  "March 17, 1999 (Wednesday, 15:27h)"
created  "March 17, 1999 (Wednesday, 15:27h)"

sample {
     __^__                 __^__
    ( ___ )---------------( ___ )
     | / |                 | \\ |
     | / |                 | \\ |
     | / |                 | \\ |
     |___|                 |___|
    (_____)---------------(_____)
}

shapes {
    nw  (" __^__ ", "( ___ )")
    nnw (" ","-")
    ne  (" __^__ ", "( ___ )")
    e   (" | / | ")
    w   (" | \\ | ")
    wsw (" |___| ")
    ese (" |___| ")
    sw  ("(_____)")
    s   ("-")
    se  ("(_____)")
}

elastic (nnw, e, w, s)

END columns


