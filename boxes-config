BOX diamonds

author "Joan G. Stark <spunk1111@juno.com>"
revision "1.0"
revdate "March 30, 1999 (Tuesday, 15:59h)"
created "March 30, 1999 (Tuesday, 15:59h)"

sample {
           /\\          /\\          /\\          /\\  
        /\\//\\\\/\\    /\\//\\\\/\\    /\\//\\\\/\\    /\\//\\\\/\\  
     /\\//\\\\\\///\\\\/\\//\\\\\\///\\\\/\\//\\\\\\///\\\\/\\//\\\\\\///\\\\/\\
    //\\\\\\//\\/\\\\///\\\\\\//\\/\\\\///\\\\\\//\\/\\\\///\\\\\\//\\/\\\\///\\\\
    \\\\//\\/                                        \\/\\\\//
     \\/       joan stark   spunk1111@juno.com        \\/
     /\\     http://www.geocities.com/SoHo/7373/      /\\
    //\\\\/\\                                        /\\//\\\\
    \\\\///\\\\/\\//\\\\\\///\\\\/\\//\\\\\\///\\\\/\\//\\\\\\///\\\\/\\//\\\\\\//
     \\/\\\\///\\\\\\//\\/\\\\///\\\\\\//\\/\\\\///\\\\\\//\\/\\\\///\\\\\\//\\/
        \\/\\\\//\\/    \\/\\\\//\\/    \\/\\\\//\\/    \\/\\\\//\\/
           \\/          \\/          \\/          \\/
}

shapes {
    nw  ("      ", "    /\\", " /\\//\\", "//\\\\\\/")
    nnw (" /\\     ", "//\\\\/\\  ", "\\\\///\\\\/", "/\\/\\\\///")
    n   ("     /\\     ", "  /\\//\\\\/\\  ", "\\//\\\\\\///\\\\/",
         "\\\\\\//\\/\\\\///")
    nne ("     /\\ ", "  /\\//\\\\", "\\//\\\\\\//", "\\\\\\//\\/\\")
    ne  ("      ", "/\\    ", "/\\\\/\\ ", "\\///\\\\")

    ene ("\\/\\\\//", "   \\/ ")
    e   ("   /\\ ", "  //\\\\", "  \\\\//", "   \\/ ")
    ese ("   /\\ ", "/\\//\\\\")

    wnw ("\\\\//\\/", " \\/   ")
    w   (" /\\   ", "//\\\\  ", "\\\\//  ", " \\/   ")
    wsw (" /\\   ", "//\\\\/\\")

    sw  ("\\\\///\\", " \\/\\\\/", "    \\/", "      ")
    ssw ("\\/\\//\\\\\\", "//\\\\\\//\\", "\\\\//\\/  ", " \\/     ")
    s   ("///\\\\/\\//\\\\\\", "/\\\\///\\\\\\//\\", "  \\/\\\\//\\/  ",
         "     \\/     ")
    sse ("///\\\\/\\/", "/\\\\///\\\\", "  \\/\\\\//", "     \\/ ")
    se  ("/\\\\\\//", "\\//\\/ ", "\\/    ", "      ")
}

elastic (n,s,e, w)

END diamonds


