BOX peek

revision "1.0"
revdate "April 12, 1999 (Monday, 12:52h)"
created "April 12, 1999 (Monday, 12:52h)"

sample {
   /*       _\\|/_
            (o o)
    +----oOO-{_\}-OOo---------------------+
    |                                    |
    | C function headers?                |
    |                                    |
    |                                    |
    +------------------------------------*/
}

shapes {
    nw  ("/*", "  ", " +")
    nnw ("       _\\|/_   ",
         "       (o o)   ",
         "----oOO-{_}-OOo")
    n   (" ", " ", "-")
    ne  (" ", " ", "+")
    e   ("|")
    w   (" |")
    sw  (" +")
    s   ("-")
    se  ("+")
}

replace "\\*/" with "*\\/"               # quote closing comment tags

elastic (n,e,s,w)

END peek


